{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:import/warnings"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "ignorePatterns": [
        "temp.js", "node_modules/"
    ],
    "rules": {
        "indent": ["error", 4],
        "import/extensions": ["off"],
        "comma-dangle": ["error", "never"],
        "space-before-function-paren": ["error", "never"],
    }
}